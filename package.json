{
  "root": true,
  "name": "@we_thoughts/thoughts-wc-test",
  "version": "0.0.4",
  "description": "Test of generator-thoughts-wc-polymer &amp; thoughts-wc-polymer.",
  "keywords": [
    "web components",
    "polymer",
    "thoughts",
    "test"
  ],
  "homepage": "https://thoughts.vip",
  "author": "cigaret <kcigaret@outlook.com> (https://thoughts.vip)",
  "main": "./build/thoughts-wc-test.js",
  "dependencies": {
    "@polymer/polymer": "^3.0.0"
  },
  "scripts": {
    "init": "git init && git add . && git commit -m initialize && git remote add origin git@github.com:we-thoughts/thoughts-wc-test.git ",
    "push": "git push -u origin master",
    "serve": "polymer serve",
    "test": "polymer test",
    "lint": "npx eslint ./",
    "lint:fix": "npx eslint ./ --fix",
    "build": "npx tsc && cp ./build/%npm_package_config_wc_name%.js %npm_package_config_wc_name%.js",
    "dist": "npx tsc && webpack --mode development --config webpack.config.js"
  },
  "devDependencies": {
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/test-fixture": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.3",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "wct-mocha": "^1.0.1",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6"
  },
  "config": {
    "wc_name": "thoughts-wc-test",
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
